pipeline
{
	agent any

	stages
	{
		stage('Welcome Stage')
		{
			steps
			{
				echo 'Welcome to Jenkins Pipeline'
			}
		}
		
		stage('Clean Stage')
		{
			steps
			{
				echo 'This is a Clean Stage'
			}
		}	
        stage('Clean Success Stage')
		{
			steps
			{
				echo 'clean success'
			}
		}	
		stage('Test Stage')
		{
			steps
			{
				echo 'This is a Test Stage'
			}
		}		
		stage('Build & Install Stage')
		{
			steps
			{
				echo 'This is a Build success stage'
			}
		}		
		
		stage('Build Success')
		{
			steps
			{
				echo 'Build successful'
			}
		}		
      }
}	
